{
  "extends": "airbnb",
  "rules": {
    "func-names": ["warn", "never"],
    "no-console": "off",
    "indent": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "padded-blocks": "off",
    "import/prefer-default-export": "off",
    "prefer-destructuring": "off",
    "no-multiple-empty-lines": "off"
  }
}